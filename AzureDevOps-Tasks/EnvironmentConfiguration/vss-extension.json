{
    "manifestVersion": 1,
    "id": "EnvironmentConfiguration",
    "name": "Generate Environment Configuration",
    "version": "0.0.29",
    "publisher": "chelnak",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Generate and validate configuration from a given schema.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "EnvironmentConfiguration"
            }
        }
    ]
}
